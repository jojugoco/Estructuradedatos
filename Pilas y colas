days=[]
dayscola=[]

days.append('Lunes')
days.append('Martes')
days.append('Miercoles')
days.append('Jueves')
days.append('Viernes')
days.append('Sabado')
days.append('Domingo')

print(days)

l = len(days)
for i in range(1):
    x=days.pop()
    print(days)
    dayscola.append(x)
    
print(dayscola)
